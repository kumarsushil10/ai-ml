     "output_type": "execute_result"
    }
   ],
   "source": [
    "def isPalindrome(input_str):\n",
    "    for i in range(len(input_str)):\n",
    "        if input_str[i] != input_str[-(i+1)]:\n",
    "            return False\n",
    "    return True\n",
    "\n",
    "\n",
    "input_str = \"Queen\"\n",
    "isPalindrome(input_str)"
   ]
